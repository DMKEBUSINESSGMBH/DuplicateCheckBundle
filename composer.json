{
    "name": "dmk/duplicate-check-bundle",
    "description": "Finds duplicate in you CRM",
    "license": "MIT",
    "type": "bundle",
    "autoload": {
        "psr-4": {
            "DMK\\DuplicateCheckBundle\\": ""
        }
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMK\\DuplicateCheckBundle\\Tests\\": "Tests/"
        }
    },
    "require": {
        "oro/platform": "^2.6|^3.0",
        "beberlei/doctrineextensions": "^1.2",
        "phpstan/extension-installer": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "phpstan/phpstan": "^0.11",
        "jangregor/phpstan-prophecy": "^0.4",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-deprecation-rules": "^0.11",
        "phpstan/phpstan-symfony": "^0.11",
        "phpstan/phpstan-doctrine": "^0.11",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "suggest": {
        "oro/crm": "^2.6|^3.0"
    },
    "authors": [
        {
            "name": "Markus Bachmann DMK",
            "email": "markus.bachmann@dmk-ebusiness.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
